vip1_addr="10.10.12.102"
vip1_port="80"
backend2_addr="10.10.10.51"
backend2_port="80"

## TCP port relay and forwarder
#
protocol "tcp_service" {
   tcp { nodelay, socket buffer 65536 }
}

relay "tcp_forwarder" {
       listen on $box1_addr port $box1_port
       protocol "tcp_service"
       forward to $backend2_addr port $backend2_port
}

